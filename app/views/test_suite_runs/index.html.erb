<h1>Listing test_suite_runs</h1>

<table>
  <thead>
    <tr>
      <th>Start time</th>
      <th>End time</th>
      <th>Pass</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @test_suite_runs.each do |test_suite_run| %>
      <tr>
        <td><%= test_suite_run.start_time %></td>
        <td><%= test_suite_run.end_time %></td>
        <td><%= test_suite_run.did_pass? %></td>
        <td><%= link_to 'Show', test_suite_test_suite_run_path(@test_suite,test_suite_run) %></td>
        <td><%= link_to 'Destroy', test_suite_test_suite_run_path(@test_suite,test_suite_run), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test suite run', new_test_suite_test_suite_run_path %>
